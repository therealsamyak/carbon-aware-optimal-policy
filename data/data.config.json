{
  "system_constants": {
    "task_interval_seconds": 300,
    "horizon_seconds": 86400,
    "battery_discretization_step": 0.01,
    "nearest_neighbor_k": 3
  },
  "user_parameters": [
    { "accuracy_threshold": 0.5, "latency_threshold_seconds": 0.008 },
    { "accuracy_threshold": 0.8, "latency_threshold_seconds": 0.006 },
    { "accuracy_threshold": 0.9, "latency_threshold_seconds": 0.01 }
  ],
  "reward_weights": [
    { "success_weight": 10, "small_miss_weight": 5, "large_miss_weight": 8, "carbon_weight": 7 },
    { "success_weight": 15, "small_miss_weight": 3, "large_miss_weight": 10, "carbon_weight": 5 },
    { "success_weight": 8, "small_miss_weight": 8, "large_miss_weight": 6, "carbon_weight": 12 },
    { "success_weight": 12, "small_miss_weight": 4, "large_miss_weight": 9, "carbon_weight": 8 }
  ],
  "battery_configs": [
    { "battery_capacity_mwh": 200, "charge_rate_mwh_per_second": 0.000008 },
    { "battery_capacity_mwh": 300, "charge_rate_mwh_per_second": 0.00001 },
    { "battery_capacity_mwh": 600, "charge_rate_mwh_per_second": 0.000012 }
  ],
  "seasonal_dates": ["2024-01-15", "2024-04-15", "2024-07-15", "2024-10-15"],
  "locations": [
    "US-CAL-LDWP_2024_5_minute.csv",
    "US-FLA-FPL_2024_5_minute.csv",
    "US-NW-PSEI_2024_5_minute.csv",
    "US-NY-NYIS_2024_5_minute.csv"
  ],
  "output": {
    "data_split": { "train": 0.7, "val": 0.1, "test": 0.2 },
    "max_workers": 90,
    "output_dir": "training_data",
    "chunk_size": 10
  }
}
